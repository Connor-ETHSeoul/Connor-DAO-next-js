
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model Agent_output {
  id         Int      @id @default(autoincrement())
  proposalId Int
  color      String
  text       String
}

model Smart_contract {
  id      Int      @id @default(autoincrement())
  version String 
  code    String
}